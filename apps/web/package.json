{
  "name": "@qademo/web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev",
    "build": "pnpm run build:frontend && pnpm run build:worker",
    "build:frontend": "tsc -b && vite build",
    "build:worker": "tsc -p tsconfig.worker.json",
    "preview": "vite preview",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "deploy": "pnpm run build:frontend && wrangler deploy --upload-source-maps",
    "deploy:fast": "vite build && wrangler deploy --upload-source-maps",
    "clean": "rm -rf dist .wrangler",
    "types:worker": "wrangler types"
  },
  "dependencies": {
    "@qademo/shared": "workspace:*",
    "@sentry/cloudflare": "^10.34.0",
    "@tanstack/react-query": "^5.40.0",
    "framer-motion": "^11.2.0",
    "hono": "^4.4.0",
    "jose": "^5.2.0",
    "lucide-react": "^0.379.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^6.23.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@eslint/js": "^9.0.0",
    "@hono/zod-validator": "^0.2.0",
    "@playwright/test": "^1.44.0",
    "@sentry/cli": "^3.1.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.0",
    "typescript-eslint": "^7.0.0",
    "vite": "^5.2.0",
    "vitest": "^1.6.0",
    "wrangler": "^4.54.0"
  }
}
